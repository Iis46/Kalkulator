print("Выберите математическое действие:Сложение,Вычитание,Умножение,Деление,Вывести из квадратного корня,Логарифм,Возведение в степень,Округление в большую сторону,Округление в меньшую сторону")
a=(input())
import math
if a=='Сложение':
    print("Введите первое слагаемое")
    a1=int(input())
    print("Введите второе слагаемое")
    a2=int(input())
    s=a1+a2
    print(s)
elif a=='Вычитание':
    print('Введите первое слагаемое')
    b=int(input())
    print('Введите второе слагаемое')
    b1=int(input())
    f=b-b1
    print(f)
elif a=='Умножение':
    print('Введите первое число')
    c=int(input())
    print('Введите второе число')
    c1=int(input())
    g=c*c1
    print(g)
elif a=='Деление':
    print('Введите перове число')
    d=int(input())
    print('Введите второе число')
    d1=int(input())
    if d1==0:
        print('Деление невозможно')
    else:
        h=d//d1
        print(h)
elif a=='Возведение в степень':
    print('Введите число')
    e=int(input())
    print('Введите степень в которую хотите возвести число')
    e1=int(input())
    j=e**e1
    print(j)
elif a=='Логарифм':
    print('Введите число')
    r=int(input())
    print('Введите основание логарифма')
    r1=int(input())
    k=math.log(r,r1)
    print(k)
elif a=='Вывести из квадратного корня':
    print('Введите число')
    t=int(input())
    l=math.sqrt(t)
    print(l)
elif a=='Округление в большую сторону':
    print('Введите число')
    y=float(input())
    q=math.ceil(y)
    print(q)
elif a=='Округление в меньшую сторону':
    print('Введите число')
    z=float(input())
    w=math.floor(z)
    print(w)
